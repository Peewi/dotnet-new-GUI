<Page x:Class="dotnet_new_GUI.Results"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:dotnet_new_GUI"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
	  d:Background="White"
      Title="Results">

    <Grid>
		<TextBox x:Name="ResultsOutput" HorizontalAlignment="Stretch" Margin="10,10,10,40" Text="" TextWrapping="Wrap" VerticalAlignment="Stretch" IsReadOnly="True"/>
		<Button x:Name="NewProjectBtn" Content="New project" HorizontalAlignment="Left" Margin="10,0,0,10" VerticalAlignment="Bottom" Padding="3,1,3,1" Click="NewProjectBtn_Click"/>

	</Grid>
</Page>
